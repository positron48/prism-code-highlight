{
    "name": "stevenknibbs/googletagmanager",
    "description": "A Google Tag Manager extension for Bolt.",
    "type": "bolt-extension",
    "license": "MIT",
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "authors": [
        {
            "name": "Steven Knibbs",
            "email": "stevenknibbs@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\StevenKnibbs\\GoogleTagManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\StevenKnibbs\\GoogleTagManager\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\StevenKnibbs\\GoogleTagManager\\GoogleTagManagerExtension"
    }
}
